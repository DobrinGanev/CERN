{
  "name": "CERN",
  "description": "Cassandra, Express, React-Redux and NodeJS.",
  "version": "0.0.0",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/DobrinGanev/CERN.git"
  },
  "homepage": "https://github.com/DobrinGanev/CERN",
  "keywords": [
    "react",
    "react-router",
    "isomorphic",
    "starter",
    "boilerplate",
    "template",
    "webpack",
    "redux",
    "redux-dev-tools",
    "Cassandra database",
    "Express"
  ],
  "main": "dist/server.js",
  "scripts": {
    "start": "forever --minUptime 1000 --spinSleepTime 1000 -c \"node --harmony\" ./dist/server.js",
    "build-server": "webpack --colors --display-error-details --config configs/webpack.server.js",
    "build-client": "webpack --colors --display-error-details --config configs/webpack.client.js",
    "build": "concurrently \"npm run build-server\" \"npm run build-client\"",
    "watch-server": "webpack --watch --verbose --colors --display-error-details --config configs/webpack.server-watch.js",
    "watch-server-start": "node node_modules/just-wait --pattern \"dist/*.js\" && npm run start",
    "watch-client": "webpack-dev-server --config configs/webpack.client-watch.js",
    "production": "cross-env NODE_ENV=production npm run build && cross-env NODE_ENV=production npm run start",
    "dev": "concurrently --kill-others \"npm run watch-server-start\" \"npm run watch-server\" \"npm run watch-client\"",
    "postinstall": "webpack -p --config configs/webpack.client.js"
  },
  "dependencies": {
    "cassandra-driver": "^3.5.0",
    "classnames": "^2.2.5",
    "cross-env": "^2.0.0",
    "dotenv": "^2.0.0",
    "forever": "^0.15.2",
    "inert": "^4.0.1",
    "isomorphic-fetch": "^2.2.1",
    "material-design-lite": "^1.2.1",
    "piping": "1.0.0-rc.3",
    "radium": "^0.18.1",
    "react": "^15.2.1",
    "react-dom": "^15.2.1",
    "react-redux": "^4.4.5",
    "react-router": "^2.6.0",
    "react-router-redux": "^4.0.5",
    "react-tap-event-plugin": "1.0.0",
    "redux": "^3.5.2",
    "redux-thunk": "^2.1.0"
  },
  "devDependencies": {
    "babel": "^6.5.2",
    "babel-core": "^6.13.2",
    "babel-loader": "^6.2.5",
    "babel-polyfill": "^6.13.0",
    "babel-preset-es2015": "^6.13.2",
    "babel-preset-react": "^6.11.1",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.14.0",
    "babel-root-import": "^4.1.1",
    "concurrently": "^2.2.0",
    "css-loader": "^0.25.0",
    "enzyme": "^2.4.1",
    "eslint": "^5.2.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-config-prettier-standard": "^2.0.0",
    "eslint-config-react-app": "^2.1.0",
    "eslint-config-standard": "^11.0.0",
    "eslint-loader": "^2.1.0",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-node": "^7.0.1",
    "eslint-plugin-prettier": "^2.6.2",
    "eslint-plugin-promise": "^3.8.0",
    "eslint-plugin-react": "^6.10.3",
    "eslint-plugin-standard": "^3.1.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "isomorphic-style-loader": "^1.0.0",
    "json-loader": "^0.5.4",
    "just-wait": "1.0.5",
    "nock": "^8.0.0",
    "node-sass": "^3.10.1",
    "postcss-loader": "^0.13.0",
    "prettier": "^1.13.7",
    "react-addons-test-utils": "^15.3.1",
    "react-hot-loader": "^1.3.0",
    "redux-logger": "^2.6.1",
    "sass-loader": "^4.0.2",
    "style-loader": "^0.13.1",
    "webpack": "^1.13.2",
    "webpack-dev-server": "^1.16.1",
    "webpack-node-externals": "^1.4.3"
  },
  "engines": {
    "node": ">=4.0.0"
  },
  "lint-staged": {
    "*.js": [
      "prettier --trailing-comma es5 --single-quote --write",
      "git add"
    ],
    "yarn.lock": [
      "git rm --cached"
    ]
  },
  "babel": {
    "presets": [
      "react"
    ]
  }
}
